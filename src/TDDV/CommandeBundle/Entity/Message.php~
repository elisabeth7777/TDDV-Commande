<?phpnamespace TDDV\CommandeBundle\Entity;use Doctrine\ORM\Mapping as ORM;/** * Message * * @ORM\Table(name="tddv_message") * @ORM\Entity(repositoryClass="TDDV\CommandeBundle\Entity\MessageRepository") */class Message{    /**     * @var integer     *     * @ORM\Column(name="id", type="integer")     * @ORM\Id     * @ORM\GeneratedValue(strategy="AUTO")     */    private $id;    /**     * @var integer     *     * @ORM\Column(name="NumeroCd", type="integer")     */    private $numeroCd;   /**    * @ORM\ManyToOne(targetEntity="TDDV\CommandeBundle\Entity\Predicateur")    * @ORM\JoinColumn(nullable=false)    */    private $predicateur;      /**     * @var \DateTime     *     * @ORM\Column(name="DateShalom", type="date", nullable=true)     */    private $dateShalom;       /**     * @var string     *     * @ORM\Column(name="typeDateShalom", type="string", length=4)     */    private $typeDateShalom;    /**     * @var \DateTime     *     * @ORM\Column(name="DateTDDV", type="date")     */    private $dateTDDV;    /**     * @var string     *     * @ORM\Column(name="typeDateTDDV", type="string", length=4)     */    private $typeDateTDDV;        /**     * @var string     *     * @ORM\Column(name="Titre", type="string", length=255)     */    private $titre;    /**     * @var string     *     * @ORM\Column(name="Type", type="string", length=255)     */    private $type;    /**     * Get id     *     * @return integer      */    public function getId()    {        return $this->id;    }    /**     * Set dateShalom     *     * @param \DateTime $dateShalom     * @return Predication     */    public function setDateShalom($dateShalom)    {        $this->dateShalom = $dateShalom;            return $this;    }    /**     * Get dateShalom     *     * @return \DateTime      */    public function getDateShalom()    {        return $this->dateShalom;    }    /**     * Set dateTDDV     *     * @param \DateTime $dateTDDV     * @return Predication     */    public function setDateTDDV($dateTDDV)    {        $this->dateTDDV = $dateTDDV;            return $this;    }    /**     * Get dateTDDV     *     * @return \DateTime      */    public function getDateTDDV()    {        return $this->dateTDDV;    }    /**     * Set titre     *     * @param string $titre     * @return Predication     */    public function setTitre($titre)    {        $this->titre = $titre;            return $this;    }    /**     * Get titre     *     * @return string      */    public function getTitre()    {        return $this->titre;    }    /**     * Set predicateur     *     * @param \TDDV\CommandeBundle\Entity\Predicateur $predicateur     * @return Predication     */    public function setPredicateur(\TDDV\CommandeBundle\Entity\Predicateur $predicateur)    {        $this->predicateur = $predicateur;            return $this;    }    /**     * Get predicateur     *     * @return \TDDV\CommandeBundle\Entity\Predicateur      */    public function getPredicateur()    {        return $this->predicateur;    }    /**     * Set type     *     * @param string $type     * @return Message     */    public function setType($type)    {        $this->type = $type;            return $this;    }    /**     * Get type     *     * @return string      */    public function getType()    {        return $this->type;    }    /**     * Set typeDateShalom     *     * @param string $typeDateShalom     * @return Message     */    public function setTypeDateShalom($typeDateShalom)    {        $this->typeDateShalom = $typeDateShalom;            return $this;    }    /**     * Get typeDateShalom     *     * @return string      */    public function getTypeDateShalom()    {        return $this->typeDateShalom;    }    /**     * Set typeDateTDDV     *     * @param string $typeDateTDDV     * @return Message     */    public function setTypeDateTDDV($typeDateTDDV)    {        $this->typeDateTDDV = $typeDateTDDV;            return $this;    }    /**     * Get typeDateTDDV     *     * @return string      */    public function getTypeDateTDDV()    {        return $this->typeDateTDDV;    }}