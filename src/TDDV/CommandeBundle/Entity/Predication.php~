<?phpnamespace TDDV\CommandeBundle\Entity;use Doctrine\ORM\Mapping as ORM;/** * Predication * * @ORM\Table() * @ORM\Entity(repositoryClass="TDDV\CommandeBundle\Entity\PredicationRepository") */class Predication{    /**     * @var integer     *     * @ORM\Column(name="id", type="integer")     * @ORM\Id     * @ORM\GeneratedValue(strategy="AUTO")     */    private $id;        /**     * @ORM\OneToOne(targetEntity="TDDV\CommandeBundle\Entity\Message")     * @ORM\JoinColumn(nullable=false)     */    private $message;        /**     * Get id     *     * @return integer      */    public function getId()    {        return $this->id;    }}